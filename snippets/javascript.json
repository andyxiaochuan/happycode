	"Print vue": {
        "prefix": "happy",
        "body": [
			"<template>",
			"\t<div class='$2'>",
			"",
			"\t</div>",
			"</template>",
			"<script>",
			"\texport default{",
			"\t\tname:'$1',",
			"\t\tcomponents:{",
			"\t\t},",
			"\t\tdata(){",
			"\t\t\treturn{};",
			"\t\t},",
			"\t\t//计算属性",
			"\t\tcomputed:{},",
			"\t\t//监控data中的数据变化",
			"\t\twatch:{},",
			"\t\t//方法集合",
			"\t\tmethods:{},",
			"\t\t//生命周期 - 创建之前",
			"\t\tbeforeCreate(){},",
			"\t\t//生命周期 - 创建完成（可以访问当前this实例）",
			"\t\tcreated(){},",
			"\t\t//生命周期 - 挂载之前",
			"\t\tbeforeMount(){},",
			"\t\t//生命周期 - 挂载完成（可以访问DOM元素）",
